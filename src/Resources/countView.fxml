<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="280.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CountViewController">
   <children>
      <VBox prefHeight="400.0" prefWidth="291.0">
         <children>
            <Pane prefHeight="62.0" prefWidth="330.0" style="-fx-background-color: rgb(50,50,50);">
               <children>
                  <Label layoutX="48.0" layoutY="20.0" text="Country-wise Player Count" textFill="#e1e1e1">
                     <font>
                        <Font name="Impact" size="18.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <TableView fx:id="tableView" prefHeight="292.0" prefWidth="290.0" />
            <Button fx:id="close" mnemonicParsing="false" onAction="#onClick" prefHeight="46.0" prefWidth="295.0" text="Close" />
         </children>
      </VBox>
   </children>
</AnchorPane>
