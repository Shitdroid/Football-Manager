<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="251.0" prefWidth="397.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ClubCountryInputController">
    <children>
        <Label fx:id="label" layoutX="35.0" layoutY="36.0" prefHeight="27.0" prefWidth="326.0" text="Please Input the Club &amp; Country Name" textFill="GAINSBORO">
            <font>
                <Font name="Impact" size="21.0" />
            </font>
        </Label>
        <TextField fx:id="cLubInput" layoutX="115.0" layoutY="86.0" onAction="#enterClub" prefHeight="26.0" prefWidth="240.0" />
        <Button fx:id="reset" layoutX="125.0" layoutY="183.0" mnemonicParsing="false" onAction="#clickReset" text="Reset" AnchorPane.leftAnchor="125.0" />
        <Button fx:id="submit" layoutX="208.0" layoutY="183.0" mnemonicParsing="false" onAction="#clickSubmit" text="Submit" AnchorPane.rightAnchor="125.0" />
      <TextField fx:id="countryInput" layoutX="115.0" layoutY="125.0" onAction="#enterCountry" prefHeight="26.0" prefWidth="240.0" />
      <Label layoutX="37.0" layoutY="90.0" text="Club Name:" textFill="GAINSBORO">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="17.0" layoutY="129.0" text="Country Name:" textFill="GAINSBORO">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
    </children>

</AnchorPane>
