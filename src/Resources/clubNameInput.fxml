<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="251.0" prefWidth="368.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ClubNameInputController">
    <children>
        <Label fx:id="label" layoutX="30.0" layoutY="49.0" prefHeight="27.0" prefWidth="308.0" text="Please Input the Name of the Club" textFill="#d7d7d7">
            <font>
                <Font name="Impact" size="22.0" />
            </font>
        </Label>
        <TextField fx:id="textInput" layoutX="64.0" layoutY="112.0" onAction="#enter" prefHeight="26.0" prefWidth="240.0" />
        <Button fx:id="reset" layoutX="116.0" layoutY="183.0" mnemonicParsing="false" onAction="#clickReset" text="Reset" AnchorPane.leftAnchor="110.0" />
        <Button fx:id="submit" layoutX="208.0" layoutY="183.0" mnemonicParsing="false" onAction="#clickSubmit" text="Submit" AnchorPane.rightAnchor="110.0" />
    </children>
</AnchorPane>
